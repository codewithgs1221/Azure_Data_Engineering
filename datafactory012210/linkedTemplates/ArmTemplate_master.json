{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory012210"
		},
		"appdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'appdb'"
		},
		"datapool_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datapool_ls'"
		},
		"datalake01221_stage_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake01221_stage_ls'"
		},
		"datalake01221_stage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake012210.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory012210_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"appdb_connectionString": {
						"value": "[parameters('appdb_connectionString')]"
					},
					"datapool_ls_connectionString": {
						"value": "[parameters('datapool_ls_connectionString')]"
					},
					"datalake01221_stage_ls_accountKey": {
						"value": "[parameters('datalake01221_stage_ls_accountKey')]"
					},
					"datalake01221_stage_ls_properties_typeProperties_url": {
						"value": "[parameters('datalake01221_stage_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}